// Find longest word in the string

function find(str){
let words=str.split(" ");
let largest=" ";

for(let word of words ){

if(word.length>largest.length){
largest=word;
}
}
return largest

}
console.log(find("i love programming "))

// Remove vowels from the string
function check(val){
let words=val.split("")
let arr1=[]
for(let word of words){
    if(word!="a"&& word!="e" && word!="i" && word!="o" && word!="u"){
    arr1.push(word)    
    }
}    
 return arr1
}






console.log(check("abc"))
